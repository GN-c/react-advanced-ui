{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "rootDir": "./",
		"lib": ["es2015.promise", "es5","es2019","DOM"],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "sourceRoot": "./src",
    "inlineSourceMap": true,
    "esModuleInterop":true,
    "noFallthroughCasesInSwitch": true,
    "types": []
  }
}
